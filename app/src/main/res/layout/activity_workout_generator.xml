<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back2"
    tools:context=".WorkoutGenerator" >

    <TextView
        android:id="@+id/fullscreen_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:keepScreenOn="true"
        android:textColor="#33b5e5"
        android:textSize="50sp"
        android:textStyle="bold" />

    <ScrollView
        android:id="@+id/ScrollView01"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scrollbars="vertical"
        android:scrollbarAlwaysDrawVerticalTrack="true" >

        <RelativeLayout
            android:id="@+id/RelativeLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >


            <EditText
                android:id="@+id/height"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/age"
                android:layout_alignParentEnd="true"

                android:layout_marginTop="36dp"
                android:layout_marginEnd="20dp"
                android:width="90dip"
                android:background="@drawable/button_list_border"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="@string/height"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#A4000000"
                android:textColorHint="#A4000000"
                android:textSize="20sp">


            </EditText>

            <EditText
                android:id="@+id/age"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/toolbar3"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="44dp"

                android:layout_marginEnd="20dp"
                android:width="90dip"
                android:background="@drawable/button_list_border"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="@string/age"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#000000"
                android:textColorHint="#A4000000"
                android:textSize="20sp"
                tools:ignore="RelativeOverlap">

                <requestFocus />
            </EditText>

            <EditText
                android:id="@+id/selfassess"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/weight"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="20dp"
                android:width="90dip"
                android:background="@drawable/button_list_border"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="@string/event"
                android:importantForAutofill="no"

                android:textColorHint="#A4000000"
                android:inputType="number"
                android:textColor="#A4000000"
                android:textSize="20sp"
                tools:ignore="RelativeOverlap">

            </EditText>

            <EditText
                android:id="@+id/weight"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/height"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="20dp"
                android:width="90dip"
                android:background="@drawable/button_list_border"
                android:ems="10"
                android:gravity="center_horizontal"
                android:hint="@string/weight"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#A4000000"
                android:textColorHint="#A4000000"
                android:textSize="20sp">

            </EditText>

            <EditText
                android:id="@+id/GeneratorList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Generate"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="82dp"
                android:layout_marginEnd="24dp"
                android:width="300dip"
                android:background="@drawable/generator_list_border"
                android:ems="10"
                android:focusable="false"
                android:importantForAutofill="no"
                android:inputType="textFilter|textMultiLine"
                android:lines="110"
                android:longClickable="false"
                android:textColor="#000000"
                android:textIsSelectable="false"
                tools:ignore="LabelFor" />




            <Button
                android:id="@+id/Generate"
                android:layout_width="220dp"
                android:layout_height="64dp"
                android:layout_below="@+id/spinner2"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="16dp"
                android:background="@drawable/wg_button_bg"
                android:text="@string/generate"
                android:textColor="#000000"
                android:textSize="25sp" />

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#CDDC39"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/toolbar3"
                android:layout_alignParentStart="true"
                android:layout_marginStart="30dp"
                android:layout_marginTop="45dp"
                android:text="Age"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView5"
                android:layout_alignParentStart="true"
                android:layout_marginStart="30dp"
                android:layout_marginTop="37dp"
                android:text="Height"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView6"
                android:layout_alignParentStart="true"
                android:layout_marginStart="30dp"
                android:layout_marginTop="39dp"
                android:text="Weight"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView7"
                android:layout_alignParentStart="true"
                android:layout_marginStart="30dp"
                android:layout_marginTop="38dp"
                android:text="1-10"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView8"
                android:layout_alignParentStart="true"
                android:layout_marginStart="30dp"
                android:layout_marginTop="38dp"
                android:text="Event"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView9"
                android:layout_alignParentStart="true"
                android:layout_marginStart="30dp"
                android:layout_marginTop="41dp"
                android:text="Month"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView10"
                android:layout_alignParentStart="true"
                android:layout_marginStart="30dp"
                android:layout_marginTop="33dp"
                android:text="Days"
                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:textStyle="bold" />

            <Spinner
                android:id="@+id/spinner"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/selfassess"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/blue_outline"
                android:entries="@array/cities_array"
                android:popupBackground="@drawable/blue_outline_white_background"
                android:spinnerMode="dropdown"
                tools:listitem="@android:layout/simple_spinner_item" />

            <Spinner
                android:id="@+id/spinner1"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/spinner"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="20dp"
                android:background="@drawable/blue_outline"
                android:popupBackground="@drawable/blue_outline_white_background"
                android:dropDownSelector="@android:drawable/btn_dropdown"
                android:entries="@array/month_array"
                tools:listitem="@android:layout/simple_spinner_item" />

            <Spinner
                android:id="@+id/spinner2"
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/spinner1"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="20dp"

                android:dropDownSelector="@android:drawable/btn_dropdown"
                android:entries="@array/days_array"
                android:background="@drawable/blue_outline"
                android:popupBackground="@drawable/blue_outline_white_background"
                tools:listitem="@android:layout/simple_spinner_item" />

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="290dp"
                android:background="@null"
                app:srcCompat="@drawable/backbb"
                android:contentDescription="TODO" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="295dp"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/toolbar3"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="65dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="-1dp"
                android:fontFamily="@font/aclonica"
                android:text="Workout Generator"
                android:textColor="#000000"
                android:textSize="24sp" />


        </RelativeLayout>
    </ScrollView>

</FrameLayout>